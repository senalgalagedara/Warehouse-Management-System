<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                             http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">

	<!-- ========== REPORT SERVLETS ========== -->

	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>InsertReportServlet</servlet-name>
		<servlet-class>OSM.servlet.InsertReportServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>InsertReportServlet</servlet-name>
		<url-pattern>/InsertReportServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>UpdateReportServlet</servlet-name>
		<servlet-class>OSM.servlet.UpdateReportServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>UpdateReportServlet</servlet-name>
		<url-pattern>/UpdateReportServlet</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>DeleteReportServlet</servlet-name>
		<servlet-class>OSM.servlet.DeleteReportServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteReportServlet</servlet-name>
		<url-pattern>/DeleteReportServlet</url-pattern>
	</servlet-mapping>



</web-app>
